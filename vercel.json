{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "ignore": ["geneSTRUCTURE_v2/**"]
    },
    {
      "src": "package.json",
      "use": "@vercel/next",
      "ignore": ["geneSTRUCTURE_v2/**"]
    }
  ],
  "routes": [
    {
      "src": "/api/py/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
